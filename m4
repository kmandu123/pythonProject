CREATE TABLE "RESUME_EMPLOYEE_SKILL_DEV_CD" ("ID" NUMBER(11) GENERATED BY DEFAULT ON NULL AS IDENTITY NOT NULL PRIMARY KEY, "EMPLOYEE_ID" NUMBER(11) NOT NULL, "COMM_CODE_ID" NUMBER(11) NOT NULL);
