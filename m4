--
-- Create model Comm_code
--
CREATE TABLE "RESUME_COMM_CODE" ("COMM_CODE" NUMBER(11) GENERATED BY DEFAULT ON NULL AS IDENTITY NOT NULL PRIMARY KEY, "COMM_CODE_NAME" NVARCHAR2(200) NULL, "REF_FIELD" NVARCHAR2(200) NULL, "DISPLAY_ORDER" NUMBER(11) NULL, "USE_YN" NVARCHAR2(1) NULL, "SUMMARY" NVARCHAR2(2000) NULL, "CREATE_DT" TIMESTAMP NULL, "UPDATE_DT" TIMESTAMP NULL, "CREATE_ID" NVARCHAR2(200) NULL, "UPDATE_ID" NVARCHAR2(200) NULL);
--
-- Create model Comm_div
--
CREATE TABLE "RESUME_COMM_DIV" ("COMM_DIV_ID" NUMBER(11) GENERATED BY DEFAULT ON NULL AS IDENTITY NOT NULL PRIMARY KEY, "COMM_DIV_NAME" NVARCHAR2(200) NULL, "SUMMARY" NVARCHAR2(2000) NULL, "CREATE_DT" TIMESTAMP NULL, "UPDATE_DT" TIMESTAMP NULL, "CREATE_ID" NVARCHAR2(200) NULL, "UPDATE_ID" NVARCHAR2(200) NULL);
--
-- Create model Education
--
CREATE TABLE "RESUME_EDUCATION" ("EDU_ID" NUMBER(11) GENERATED BY DEFAULT ON NULL AS IDENTITY NOT NULL PRIMARY KEY, "EDU_NAME" NVARCHAR2(200) NULL, "EDU_START_DATE" DATE NULL, "EDU_END_DATE" DATE NULL, "REQUEST_YN" NVARCHAR2(1) NULL, "AGENCY_NAME" NVARCHAR2(200) NULL, "SUMMARY" NVARCHAR2(2000) NULL, "CREATE_DT" TIMESTAMP NULL, "UPDATE_DT" TIMESTAMP NULL, "CREATE_ID" NVARCHAR2(200) NULL, "UPDATE_ID" NVARCHAR2(200) NULL);
--
-- Create model Employee
--
CREATE TABLE "RESUME_EMPLOYEE" ("EMP_ID" NUMBER(11) GENERATED BY DEFAULT ON NULL AS IDENTITY NOT NULL PRIMARY KEY, "EMP_NAME" NVARCHAR2(200) NULL, "GENDER" NVARCHAR2(1) NULL, "BIRTHDATE" DATE NULL, "MOBILE_NO" NVARCHAR2(50) NULL, "EMERGENCY_TELNO" NVARCHAR2(50) NULL, "EMERGENCY_REL" NVARCHAR2(50) NULL, "EMAIL_ADDR" NVARCHAR2(254) NULL, "POSTNO" NVARCHAR2(50) NULL, "ADDR" NVARCHAR2(400) NULL, "ADDR_DTL" NVARCHAR2(200) NULL, "INDATE" DATE NULL, "OUTDATE" DATE NULL, "WORK_BASE_DATE" DATE NULL, "WORK_BASE_YEAR" NUMBER(11) NULL, "WORK_BASE_MONTH" NUMBER(11) NULL, "SKILL_MAIN" NVARCHAR2(2000) NULL, "SKILL_MAJOR" NVARCHAR2(2000) NULL, "SUMMARY" NVARCHAR2(2000) NULL, "CREATE_DT" TIMESTAMP NULL, "UPDATE_DT" TIMESTAMP NULL, "CREATE_ID" NVARCHAR2(200) NULL, "UPDATE_ID" NVARCHAR2(200) NULL, "EMP_POSITION_CD" NUMBER(11) NULL, "EVIDENCE_METHOD_CD" NUMBER(11) NULL, "SKILL_GRADE_CD" NUMBER(11) NULL);
